package LearnJava;
import java.util.*;
import java.util.Arrays;

public class Practice {
	
	public static void main(String[] args) {
		int arr[] = {1,2,3,4,5};
		
		int res[] = new int[5];
		int k = 3;
		int num = 0;
		for(int i=k; i<5; i++) {
			res[num] = arr[i];
			num++;
		}
		for(int i=0; i<k; i++) {
			res[num]= arr[i];
			num++;
		}
		for(int i=0;i<5; i++) {
			System.out.print(res[i]+" ");
		}
		
		
	}

}
